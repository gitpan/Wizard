<html>
</html>
<ep-perl>
{
	my $cgi = $self->{'cgi'};
	my $mod = $cgi->param('module');
	my $module = '';
	if($mod =~ /^(.*)$/) {
	    $module = $1;
	}
	require $module; 
	my $file = $INC{$module};
	$file =~ s/\.pm$/\.pod/;
	my $htmlout = '';
	if(-f $file) {
	    $htmlout = `cd /tmp; /usr/bin/pod2html $file`;
	} else {
	    $htmlout = "No such file: $file";
	}
	$htmlout;
}	
</ep-perl>
